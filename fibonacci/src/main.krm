node HelloKarma {
    fn main() -> () {
        var matrix: [[int; 3]; 3] = [
            [1, 2, 3],
            [4, 5, 6],
            [7, 8, 9],
        ];

        var i: int = 0;
        while i < 3 {
            var j: int = 0;
            while j < 3 {
                print_int(matrix[i][j]);
                print_char(' ');

                j = j + 1;
            }
            println();

            i = i + 1;
        }
    }
}