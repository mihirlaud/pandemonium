node HelloKarma {

    fn main() -> int {
        const limit: int = 100;
        var a: int = 0;
        var b: int = 1;
        var n: int = 10;

        while n > 0 && a <= limit {
            var c: int = a + b;
            a = b;
            b = c;

            n = n - 1;
        }

        if a >= limit {
            return limit;
        } else {
            return a;
        }
    }

    fn decrement(n: int) -> int {
        return n - 1;
    }
}